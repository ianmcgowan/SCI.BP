  OPEN FILE TO FVAR ELSE STOP 201, FILE
  WFLAG=1
  READ DUMMY FROM FVAR, ITEM THEN
    IF NOT(OVERWRITE) THEN WFLAG=0 ; PRINT ITEM:" EXISTS, SKIPPED"
