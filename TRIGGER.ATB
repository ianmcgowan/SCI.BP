OVERWRITE=0
FOR F=1 TO DCOUNT(R,@AM)
  REC=R<F>
  SWAP "#254#" WITH @AM IN REC
  SWAP "#134#" WITH "\" IN REC
  FILE=REC<1> ; DEL REC<1>
  ITEM=REC<1> ; DEL REC<1>
