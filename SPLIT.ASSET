      *  DATA ""
      *END
      DATA "" ;* BLENDED LINKED TO ASSET
      DATA "" ;* ASSET FUNDING RECS
      DATA "" ;* ENTER
      DATA "N";* HARDCOPY=N
      DATA "Y";* CONTINUE=Y
      DATA "" ;* ALL DONE
      EXECUTE \SPLIT.00\
      NEW.ASSET.LIST=OCONV(CONTRACT,"TLS.MASTER,LS.BILLING;X;11;11")
      AFTER.COUNT=DCOUNT(NEW.ASSET.LIST<1>,@VM)
      PRINT NEW.ASSET.LIST:'=':AFTER.COUNT
      IF BEFORE.COUNT=AFTER.COUNT THEN
